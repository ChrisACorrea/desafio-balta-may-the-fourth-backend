{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DefaultConnectionString": "Prod",
  "ConnectionStrings": {
    "Prod": "Host=[HOST];Port=[PORT];Database=[DATABASE];Username=[USERNAME];Password=[PASSWORD];"
  }
}
